<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>OtherPhone_Prevent_Other_Characters</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK(PersonOtherPhone)),NOT(REGEX(PersonOtherPhone, &quot;^[0-9]+$&quot;)))</errorConditionFormula>
    <errorDisplayField>PersonOtherPhone</errorDisplayField>
    <errorMessage>Assicurarsi di inserire solo caratteri numerici</errorMessage>
</ValidationRule>
